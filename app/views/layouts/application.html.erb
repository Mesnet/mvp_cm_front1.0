<!DOCTYPE html>
<html>
  <head>
    <title>Cm11</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%- if user_signed_in? %>
      <%= render 'layouts/nav/nav_in' %>
      <%= render 'layouts/nav/sidenav' if @nav1 %>
      <%= render 'layouts/nav/new' if @nav1 %>
      <%= render 'layouts/nav/sidenav2' if @nav2 %>
    <% else %>
      <%= render 'layouts/nav/nav_out' %>
    <% end %>
    <div class="body_box">
      <%= yield %>
    </div>
  </body>
</html>
